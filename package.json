{
  "name": "@pubpub/editor",
  "version": "5.3.5",
  "description": "PubPub Collaborative Editor",
  "main": "dist/index.js",
  "author": "PubPub Team <pubpub@media.mit.edu>",
  "license": "GPL-2.0+",
  "repository": {
    "type": "git",
    "url": "https://github.com/pubpub/pubpub-editor.git"
  },
  "scripts": {
    "build": "npm run clean && babel src -d dist --copy-files",
    "clean": "rm -rf dist && mkdir dist",
    "install-git-hooks": "rm -f ./.git/hooks/* && cp ./.githooks/* ./.git/hooks && chmod +x ./.git/hooks/*",
    "lint": "eslint src stories .storybook",
    "prepublish-soon": "npm run lint && rm -rf node_modules && npm install && npm run build",
    "prepublishOnly": "npm run build",
    "pretty": "prettier --write '**/*.js'",
    "start": "start-storybook -p 9002 -c .storybook"
  },
  "dependencies": {
    "dom-anchor-text-quote": "^4.0.2",
    "katex": "^0.9.0",
    "prosemirror-collab": "^1.1.1",
    "prosemirror-commands": "^1.0.7",
    "prosemirror-compress-pubpub": "0.0.3",
    "prosemirror-gapcursor": "^1.0.3",
    "prosemirror-history": "^1.0.3",
    "prosemirror-inputrules": "^1.0.1",
    "prosemirror-keymap": "^1.0.1",
    "prosemirror-model": "^1.6.3",
    "prosemirror-schema-list": "^1.0.1",
    "prosemirror-schema-table": "^0.22.0",
    "prosemirror-state": "^1.2.2",
    "prosemirror-tables": "^0.7.10",
    "prosemirror-transform": "^1.1.3",
    "prosemirror-view": "^1.6.7"
  },
  "peerDependencies": {
    "firebase": "^5.1.0",
    "react": "^16.7.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.7.0",
    "react-transition-group": "^2.5.2"
  },
  "devDependencies": {
    "@blueprintjs/core": "^2.3.1",
    "@storybook/addon-options": "^4.0.0-alpha.10",
    "@storybook/react": "^4.0.0-alpha.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bourbon": "^5.0.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^1.1.11",
    "firebase": "^5.5.4",
    "node-sass": "^4.7.2",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.8.3",
